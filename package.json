{
  "name": "supermung-server",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "dev": "npm run generate-openapi-json && dotenv -e .env.development -- ts-node-dev -r tsconfig-paths/register src/app.ts",
    "generate-openapi-json": "tspec generate --specPathGlobs src/api-spec.ts --outputPath src/openapi.json",
    "start": "node ./bin/www"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "cookie-parser": "~1.4.4",
    "debug": "~2.6.9",
    "dotenv": "^17.2.3",
    "express": "~4.16.1",
    "http-status-codes": "^2.3.0",
    "jsonwebtoken": "^9.0.2",
    "morgan": "~1.9.1",
    "prisma": "^6.17.1",
    "swagger-ui-express": "^5.0.1",
    "tspec": "^0.1.116",
    "zod": "^4.1.12"
  },
  "devDependencies": {
    "@prisma/client": "^6.3.0",
    "@types/bcrypt": "^6.0.0",
    "@types/cookie-parser": "^1.4.9",
    "@types/express": "^5.0.3",
    "@types/jsonwebtoken": "^9.0.10",
    "@types/morgan": "^1.9.10",
    "@types/node": "^24.8.1",
    "@types/swagger-ui-express": "^4.1.8",
    "dotenv-cli": "^11.0.0",
    "ts-node": "^10.9.2",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.2.0",
    "tsx": "^4.20.6",
    "typescript": "^5.9.3"
  }
}
